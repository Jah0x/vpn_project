{
  "name": "vpn-service",
  "version": "1.0.0",
  "description": "Современный VPN сервис с React интерфейсом",
  "homepage": ".",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "lucide-react": "^0.263.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^3.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "tailwindcss build -i src/index.css -o public/styles.css",
    "dev": "npm start",
    "preview": "npm run build && npx serve -s build",
    "lint": "eslint src/**/*.{js,jsx}",
    "format": "prettier --write src/**/*.{js,jsx,css,md}",
    "server": "node server/index.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "vpn",
    "react",
    "frontend",
    "vless",
    "proxy",
    "privacy",
    "security"
  ],
  "author": {
    "name": "VPN Service Team",
    "email": "support@vpnservice.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vpnservice/frontend.git"
  },
  "bugs": {
    "url": "https://github.com/vpnservice/frontend/issues"
  },
  "proxy": "http://localhost:8000"
}
